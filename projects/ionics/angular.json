{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "defaultProject": "firstApp",
  "newProjectRoot": "apps",
  "projects": {
    "firstApp": {
      "root": "",
      "sourceRoot": "apps/firstApp",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/firstApp",
            "index": "apps/firstApp/index.html",
            "main": "apps/firstApp/main.ts",
            "polyfills": "apps/firstApp/polyfills.ts",
            "tsConfig": "apps/firstApp/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "apps/firstApp/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
              }
            ],
            "styles": ["apps/firstApp/theme/variables.scss", "apps/firstApp/global.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/firstApp/environments/environment.ts",
                  "with": "apps/firstApp/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "ci": {
              "progress": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "firstApp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "firstApp:build:production"
            },
            "ci": {
              "progress": false
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "firstApp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/firstApp/test.ts",
            "polyfills": "apps/firstApp/polyfills.ts",
            "tsConfig": "apps/firstApp/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "styles": [],
            "scripts": [],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "apps/firstApp/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "apps/firstApp/assets",
                "output": "/assets"
              }
            ]
          },
          "configurations": {
            "ci": {
              "progress": false,
              "watch": false
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/firstApp/tsconfig.app.json",
              "apps/firstApp/tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "firstApp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "firstApp:serve:production"
            },
            "ci": {
              "devServerTarget": "firstApp:serve:ci"
            }
          }
        },
        "ionic-cordova-build": {
          "builder": "@ionic/angular-toolkit:cordova-build",
          "options": {
            "browserTarget": "firstApp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "firstApp:build:production"
            }
          }
        },
        "ionic-cordova-serve": {
          "builder": "@ionic/angular-toolkit:cordova-serve",
          "options": {
            "cordovaBuildTarget": "firstApp:ionic-cordova-build",
            "devServerTarget": "firstApp:serve"
          },
          "configurations": {
            "production": {
              "cordovaBuildTarget": "firstApp:ionic-cordova-build:production",
              "devServerTarget": "firstApp:serve:production"
            }
          }
        }
      }
    },
    "secondApp": {
      "root": "",
      "sourceRoot": "apps/secondApp",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/secondApp",
            "index": "apps/secondApp/index.html",
            "main": "apps/secondApp/main.ts",
            "polyfills": "apps/secondApp/polyfills.ts",
            "tsConfig": "apps/secondApp/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "apps/secondApp/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
              }
            ],
            "styles": ["apps/secondApp/theme/variables.scss", "apps/secondApp/global.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/secondApp/environments/environment.ts",
                  "with": "apps/secondApp/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "ci": {
              "progress": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "secondApp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "secondApp:build:production"
            },
            "ci": {
              "progress": false
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "secondApp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/secondApp/test.ts",
            "polyfills": "apps/secondApp/polyfills.ts",
            "tsConfig": "apps/secondApp/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "styles": [],
            "scripts": [],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "apps/secondApp/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "apps/secondApp/assets",
                "output": "/assets"
              }
            ]
          },
          "configurations": {
            "ci": {
              "progress": false,
              "watch": false
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/secondApp/tsconfig.app.json",
              "apps/secondApp/tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "secondApp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "secondApp:serve:production"
            },
            "ci": {
              "devServerTarget": "secondApp:serve:ci"
            }
          }
        },
        "ionic-cordova-build": {
          "builder": "@ionic/angular-toolkit:cordova-build",
          "options": {
            "browserTarget": "secondApp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "secondApp:build:production"
            }
          }
        },
        "ionic-cordova-serve": {
          "builder": "@ionic/angular-toolkit:cordova-serve",
          "options": {
            "cordovaBuildTarget": "secondApp:ionic-cordova-build",
            "devServerTarget": "secondApp:serve"
          },
          "configurations": {
            "production": {
              "cordovaBuildTarget": "secondApp:ionic-cordova-build:production",
              "devServerTarget": "secondApp:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@ionic/angular-toolkit"
  },
  "schematics": {
    "@ionic/angular-toolkit:component": {
      "styleext": "scss"
    },
    "@ionic/angular-toolkit:page": {
      "styleext": "scss"
    }
  }
}
